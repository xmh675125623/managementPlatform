<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jiuzhou.firewall.mapper.FirewallRuleTempMapper">

	<select id="getByType" resultType="com.jiuzhou.firewall.bean.FirewallRuleTemp">
		SELECT
			*
		FROM 
			firewall_rule_temp
		WHERE 
			`type`=#{type}
	</select>
	
	<select id="getById" resultType="com.jiuzhou.firewall.bean.FirewallRuleTemp">
		SELECT
			*
		FROM 
			firewall_rule_temp
		WHERE 
			`id`=#{id}
	</select>
	
</mapper>