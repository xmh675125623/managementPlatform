<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jiuzhou.plat.mapper.AuditWhitelistRuleTemplateMapper">

	<select id="getByType" resultType="com.jiuzhou.plat.bean.AuditWhitelistRuleTemplate">
		SELECT 
			* 
		FROM 
			audit_whitelist_rule_template
		WHERE
			`type`=#{type}
		LIMIT 1
	</select>

</mapper>